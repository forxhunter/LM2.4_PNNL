                tmp(i,2) = fps{i,1,1}(2);
            elseif fps{i,1,2}(2) > 0
                tmp(i,2) = fps{i,1,2}(2);
            else
                error 'Ooops';
            end
            tmp(i,3) = fps{i,1,2}(3);
        elseif fps{i,1,1}(1) > 0 && fps{i,1,2}(1) > 0
            tmp(i,1) = fps{i,1,1}(1);
            tmp(i,2) = -1;
            tmp(i,3) = -1;
        elseif fps{i,1,1}(3) > 0 && fps{i,1,2}(3) > 0
