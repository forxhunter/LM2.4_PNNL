            a0=a0s(i); a1=a1s(i); p0=p0s(i); p1=round(fp(i,1)); p2=round(fp(i,2)); p3=round(fp(i,3)); p1i=p1+1; p2i=p2+1; p3i=p3+1; pMax=2*a1; pMaxi=pMax+1;
