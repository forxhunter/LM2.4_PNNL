                matlab-submit calc_fp "'data/matlab/fp.dat'" "'data/matlab/pdf.dat'" $((i+1)) $((j+1)) $((k+1)) "logs/calc_fp_${i}_${j}_${k}.log" sif 1 erobert3;
            done;
